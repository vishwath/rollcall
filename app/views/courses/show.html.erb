<div class="page-header">
  <h1>Course #<%= @course.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Department</dt>
  <dd><%= @course.department %></dd>

  <dt>Number</dt>
  <dd><%= @course.number %></dd>

  <dt>Title</dt>
  <dd><%= @course.title %></dd>

</dl>

<div class="col-md-2">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Held on</th>
      </tr>
    </thead>

    <tbody>
    <% @meetings.each do |meeting| %>
      <tr>
          <td><%= meeting.held_on %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
